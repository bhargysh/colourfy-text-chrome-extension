{
  "manifest_version": 2,
  "name": "Wiggle Text",
  "version": "1.0.0",
  "description": "Add a pop of colour to the forms you fill",
  "short_name": "Wiggle Text",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Choose your theme"
  }
}
